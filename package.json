{
  "name": "@ack-lab/sdk",
  "version": "0.1.0",
  "description": "Ack Lab SDK",
  "homepage": "https://github.com/catena-labs/ack-lab-sdk#readme",
  "bugs": "https://github.com/catena-labs/ack-lab-sdk/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/catena-labs/ack-lab-sdk.git"
  },
  "license": "MIT",
  "author": {
    "name": "Catena Labs",
    "url": "https://catenalabs.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "check": "pnpm check:format && pnpm check:lint && pnpm check:types && pnpm check:publint && pnpm check:examples && pnpm test",
    "check:examples": "pnpm check:examples:buyer && pnpm check:examples:paywall",
    "check:examples:buyer": "cd examples/buyer && pnpm install && pnpm run check",
    "check:examples:paywall": "cd examples/paywall && pnpm install && pnpm run check",
    "check:format": "prettier --experimental-cli --check .",
    "check:lint": "eslint .",
    "check:publint": "publint",
    "check:types": "tsc --noEmit --pretty",
    "clean": "git clean -fdX dist",
    "demo": "tsx src/demo/addition-demo.ts",
    "demo:payment-flow": "tsx src/demo/payment-flow.ts",
    "format": "prettier --experimental-cli --write .",
    "lint:fix": "eslint . --fix",
    "nuke": "pnpm clean && git clean -xdf node_modules",
    "outdated": "npx npm-check-updates --interactive --format group",
    "prepublishOnly": "pnpm run build",
    "test": "vitest"
  },
  "dependencies": {
    "agentcommercekit": "^0.10.0",
    "jose": "^6.0.12",
    "safe-stable-stringify": "^2.5.0",
    "standard-parse": "^0.3.0",
    "uuid": "^11.1.0",
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@ai-sdk/anthropic": "^2.0.4",
    "@ai-sdk/openai": "^2.0.14",
    "@ai-sdk/valibot": "^1.0.3",
    "@eslint/js": "^9.33.0",
    "@hono/node-server": "^1.18.2",
    "@hono/valibot-validator": "^0.5.3",
    "@ianvs/prettier-plugin-sort-imports": "^4.6.2",
    "@inquirer/prompts": "^7.8.2",
    "@prettier/plugin-oxc": "^0.0.4",
    "@types/node": "^24.2.1",
    "ai": "^5.0.14",
    "eslint": "^9.33.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.3.0",
    "hono": "^4.9.1",
    "prettier": "^3.6.2",
    "prettier-plugin-jsdoc": "^1.3.3",
    "prettier-plugin-packagejson": "^2.5.19",
    "publint": "^0.3.12",
    "tsdown": "^0.14.1",
    "tsx": "^4.20.4",
    "typescript": "^5",
    "typescript-eslint": "^8.39.1",
    "vitest": "^3.2.4",
    "yoctocolors": "^2.1.1",
    "zod": "^4.0.17"
  },
  "packageManager": "pnpm@10.14.0",
  "publishConfig": {
    "access": "public"
  }
}
