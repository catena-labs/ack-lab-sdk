{
  "name": "@ack-hub/sdk",
  "version": "0.0.1",
  "description": "Ack Hub SDK",
  "homepage": "https://github.com/agentcommercekit/ack-hub-sdk#readme",
  "bugs": "https://github.com/agentcommercekit/ack-hub-sdk/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agentcommercekit/ack-hub-sdk.git"
  },
  "license": "MIT",
  "author": {
    "name": "Catena Labs",
    "url": "https://catenalabs.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./a2a": {
      "types": "./dist/a2a/index.d.ts",
      "import": "./dist/a2a/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "check": "pnpm check:format && pnpm check:lint && pnpm check:types && pnpm check:publint",
    "check:format": "prettier --check .",
    "check:lint": "eslint .",
    "check:publint": "publint",
    "check:types": "tsc --noEmit --pretty",
    "clean": "git clean -fdX dist",
    "format": "prettier --write .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@a2a-js/sdk": "^0.2.2",
    "agentcommercekit": "^0.7.0",
    "jose": "^6.0.11",
    "safe-stable-stringify": "^2.5.0",
    "tool-adapters": "^0.0.2",
    "uuid": "^11.1.0",
    "valibot": "^1.1.0",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/node": "^22.15.29",
    "eslint": "^9.28.0",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.15",
    "publint": "^0.3.12",
    "tsdown": "^0.12.9",
    "typescript": "^5",
    "typescript-eslint": "^8.34.0"
  },
  "packageManager": "pnpm@10.11.1"
}
