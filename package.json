{
  "name": "@ack-hub/sdk",
  "version": "0.0.0",
  "description": "Ack Hub SDK",
  "homepage": "https://github.com/agentcommercekit/ack-hub-sdk#readme",
  "bugs": "https://github.com/agentcommercekit/ack-hub-sdk/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agentcommercekit/ack-hub-sdk.git"
  },
  "license": "MIT",
  "author": {
    "name": "Catena Labs",
    "url": "https://catenalabs.com"
  },
  "type": "module",
  "exports": {
    "./anthropic": {
      "types": "./dist/anthropic/index.d.ts",
      "import": "./dist/anthropic/index.js"
    },
    "./google": {
      "types": "./dist/google/index.d.ts",
      "import": "./dist/google/index.js"
    },
    "./vercel": {
      "types": "./dist/vercel/index.d.ts",
      "import": "./dist/vercel/index.js"
    },
    "./openai/chat": {
      "types": "./dist/openai/chat/index.d.ts",
      "import": "./dist/openai/chat/index.js"
    },
    "./openai/responses": {
      "types": "./dist/openai/responses/index.d.ts",
      "import": "./dist/openai/responses/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "check": "pnpm check:format && pnpm check:lint && pnpm check:types && pnpm check:publint",
    "check:format": "prettier --check .",
    "check:lint": "eslint .",
    "check:publint": "publint",
    "check:types": "tsc --noEmit --pretty",
    "clean": "git clean -fdX dist",
    "format": "prettier --write .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "agentcommercekit": "^0.3.0",
    "ai": "^4.3.16",
    "jose": "^6.0.11",
    "tool-adapters": "^0.0.2",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@eslint/js": "^9.28.0",
    "@types/node": "^22.15.29",
    "eslint": "^9.28.0",
    "globals": "^16.2.0",
    "prettier": "^3.5.3",
    "prettier-plugin-packagejson": "^2.5.15",
    "publint": "^0.3.12",
    "typescript": "^5",
    "typescript-eslint": "^8.34.0"
  },
  "peerDependencies": {
    "@anthropic-ai/sdk": "^0.52.0",
    "@google/genai": "^1.3.0",
    "openai": "^5.1.0"
  },
  "packageManager": "pnpm@10.11.1"
}
