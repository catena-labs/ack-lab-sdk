{
  "name": "ack-lab-sdk",
  "version": "0.0.2",
  "description": "Ack Lab SDK",
  "homepage": "https://github.com/catena-labs/ack-lab-sdk#readme",
  "bugs": "https://github.com/catena-labs/ack-lab-sdk/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/catena-labs/ack-lab-sdk.git"
  },
  "license": "MIT",
  "author": {
    "name": "Catena Labs",
    "url": "https://catenalabs.com"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./a2a": {
      "types": "./dist/a2a/index.d.ts",
      "import": "./dist/a2a/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "check": "pnpm check:format && pnpm check:lint && pnpm check:types && pnpm check:publint",
    "check:format": "prettier --experimental-cli --check .",
    "check:lint": "eslint .",
    "check:publint": "publint",
    "check:types": "tsc --noEmit --pretty",
    "clean": "git clean -fdX dist",
    "format": "prettier --experimental-cli --write .",
    "lint:fix": "eslint . --fix",
    "outdated": "npx npm-check-updates --interactive --format group"
  },
  "dependencies": {
    "@a2a-js/sdk": "0.2.2",
    "agentcommercekit": "^0.8.2",
    "jose": "^6.0.12",
    "safe-stable-stringify": "^2.5.0",
    "tool-adapters": "^0.0.2",
    "uuid": "^11.1.0",
    "valibot": "^1.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.6.1",
    "@prettier/plugin-oxc": "^0.0.4",
    "@types/node": "^24.2.1",
    "eslint": "^9.33.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.3.0",
    "prettier": "^3.6.2",
    "prettier-plugin-jsdoc": "^1.3.3",
    "prettier-plugin-packagejson": "^2.5.19",
    "publint": "^0.3.12",
    "tsdown": "^0.14.1",
    "typescript": "^5",
    "typescript-eslint": "^8.39.1"
  },
  "packageManager": "pnpm@10.14.0"
}
